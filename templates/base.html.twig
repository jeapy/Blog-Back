<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        
            <!-- OpenGraph Meta Tags -->
            <meta property="og:title" content="{{ og_title|default('Default Title') }}" />
            <meta property="og:description" content="{{ og_description|default('') }}" />
            <meta property="og:image" content="{{ og_image|default('') }}" />
            <meta property="og:url" content="{{ og_url|default('') }}" />
            <meta property="og:type" content="article" />

        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
       <link rel="stylesheet" href="{{ asset('styles/app.css') }}">
        {% block stylesheets %}
      
        {% endblock %}

        {% block javascripts %}
          {# {% block importmap %}{{ importmap('app') }}{% endblock %} #}  
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
